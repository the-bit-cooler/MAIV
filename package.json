{
  "name": "maiv",
  "version": "1.2.0",
  "private": true,
  "main": "expo-router/entry",
  "scripts": {
    "start": "npx expo start --clear --tunnel",
    "start:prod": "npx expo start --no-dev --minify --clear --tunnel",
    "lint": "npx expo lint",
    "install:check": "npx expo install --check",
    "export": "npx expo export",
    "export:clear": "npx expo export --clear",
    "env-local:generate": "bash scripts/generate-env.sh",
    "env-remote:list": "eas env:list --include-sensitive",
    "ios:preview": "npm run install:check && eas build --platform ios --profile preview",
    "ios:production": "npm run install:check && eas build --platform ios --profile production",
    "ios:submit": "eas submit --platform ios",
    "android:preview": "npm run install:check && eas build --platform android --profile preview",
    "android:production": "npm run install:check && eas build --platform android --profile production",
    "android:submit": "eas submit --platform android",
    "eas:install": "npm install -g eas-cli",
    "eas:login": "eas login",
    "eas:whoami": "eas whoami",
    "prepare": "husky"
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^4.1.1",
    "@expo/vector-icons": "^15.0.3",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/bottom-tabs": "^7.4.0",
    "@react-navigation/drawer": "^7.5.0",
    "@react-navigation/elements": "^2.6.3",
    "@react-navigation/native": "^7.1.8",
    "@shopify/flash-list": "2.0.2",
    "expo": "54.0.21",
    "expo-application": "~7.0.7",
    "expo-audio": "~1.0.14",
    "expo-clipboard": "~8.0.7",
    "expo-constants": "~18.0.10",
    "expo-font": "~14.0.9",
    "expo-haptics": "~15.0.7",
    "expo-image": "~3.0.10",
    "expo-linking": "~8.0.8",
    "expo-print": "~15.0.7",
    "expo-router": "~6.0.14",
    "expo-sharing": "~14.0.7",
    "expo-splash-screen": "~31.0.10",
    "expo-sqlite": "~16.0.8",
    "expo-status-bar": "~3.0.8",
    "expo-symbols": "~1.0.7",
    "expo-system-ui": "~6.0.8",
    "expo-web-browser": "~15.0.8",
    "marked": "^16.4.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-markdown-display": "^7.0.2",
    "react-native-modal": "^14.0.0-rc.1",
    "react-native-pager-view": "6.9.1",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "@types/react": "~19.1.0",
    "eslint": "^9.25.0",
    "eslint-config-expo": "~10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.6",
    "prettier": "^3.6.2",
    "typescript": "~5.9.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": "expo lint",
    "*.js": "expo lint"
  }
}
